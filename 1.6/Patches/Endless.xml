    <!-- Check if the EndlessGrowth mod is loaded and apply patch for MeleeDodgeChance -->
    <Operation Class="PatchOperationFindMod">
        <mods>
            <li>EndlessGrowth</li>
        </mods>
        <match Class="PatchOperationConditional">
            <!-- Check if the specific point exists in MeleeDodgeChance -->
            <xpath>Defs/StatDef[defName="MeleeDodgeChance"]/postProcessCurve/points/li[text()="(100000, 100000)"]</xpath>
            <nomatch Class="PatchOperationAdd">
                <xpath>Defs/StatDef[defName="MeleeDodgeChance"]/postProcessCurve/points</xpath>
                <value>
                    <li>(100, 1.0)</li>
                </value>
            </nomatch>
        </match>
    </Operation>

