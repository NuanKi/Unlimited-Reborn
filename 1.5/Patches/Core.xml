    <!-- ArmorRatingBase MaxValue -->
    <Operation Class="XmlExtensions.FindMod">
        <mods>
            <li>Combat Extended</li>
        </mods>
        <caseFalse>
            <Operation Class="XmlExtensions.ApplyPatch">
                <patchName>Reborn_RemoveMaxValue</patchName>
                <arguments>
                    <li>ArmorRatingBase</li>
                    <li>@Name</li>
                    <li>Armor rating base</li>
                    <li>false</li>
                    <li>Apparel</li>
                </arguments>
            </Operation>
        </caseFalse>
    </Operation>

