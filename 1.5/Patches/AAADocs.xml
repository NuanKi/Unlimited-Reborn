<?xml version="1.0" encoding="utf-8" ?>
<Patch>
    <Operation Class="XmlExtensions.CreateDocument">
        <xpath>Defs/XmlExtensions.SettingsMenuDef[defName="UnlimitedReborn"]</xpath>
        <docName>RebornMenu</docName>
    </Operation>

    <Operation Class="XmlExtensions.CreateDocument">
        <xpath>Defs/StatDef</xpath>
        <docName>StatDefs</docName>
    </Operation>

    <Operation Class="XmlExtensions.FindMod">
        <mods>
            <li>Ideology</li>
        </mods>
        <caseTrue>
            <Operation Class="XmlExtensions.CreateDocument">
                <xpath>Defs/ThingDef[defName="Plant_TreeGauranlen"]</xpath>
                <docName>GauranlenTree</docName>
            </Operation>
        </caseTrue>
    </Operation>
</Patch>